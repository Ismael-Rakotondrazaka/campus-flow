{
  "private": true,
  "scripts": {
    "build": "nitro build",
    "dev": "npm run prepare && nitro dev",
    "prepare": "nitro prepare && npx prisma generate",
    "preview": "node .output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint:js": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "db:seed": "npx prisma db push && npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force --skip-seed && npx prisma db push && npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.27",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "nitropack": "latest",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.8.0",
    "@prisma/client": "^5.11.0",
    "@types/jsonwebtoken": "^9.0.6",
    "bcrypt": "^5.1.1",
    "destr": "^2.0.3",
    "file-type": "^19.0.0",
    "http-status-codes": "^2.3.0",
    "is-type-of": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.10.58",
    "mime": "^4.0.1",
    "nanoid": "^5.0.6",
    "nodemailer": "^6.9.12",
    "slugify": "^1.6.6",
    "type-fest": "^4.12.0",
    "xss": "^1.0.15",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2",
    "zod-prisma-types": "^3.1.6"
  }
}
